generator client {
  provider = "prisma-client"
  binaryTargets = ["native", "debian-openssl-1.1.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      =  env("DATABASE_URL")
}

model ResumeDownload {
  id    Int @id @default(autoincrement()) // unique ID per row
  count Int @default(1)                   // default count is now 1
  createdAt DateTime @default(now())
}

